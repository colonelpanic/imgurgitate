{
  "author": "Matt Hickford <matt.hickford@gmail.com> (http://matt-hickford.github.com/)",
  "name": "imgurgitate",
  "description": "Download Imgur albums to disk. Download all of a Redditor's albums to disk.",
  "version": "0.0.5",
  "homepage": "https://github.com/matt-hickford/imgurgitate",
  "bugs": {
    "name": "https://github.com/matt-hickford/imgurgitate/issues"
  },
  "keywords": [
    "imgur",
    "album",
    "reddit",
    "download"
  ],
  "directories": {
    "lib": "./lib"
  },
  "main": "./lib/imgurgitate.js",
  "bin": {
    "imgurgitate": "./lib/imgurgitate.js"
  },
  "scripts": {
    "prepublish": "icake build",
    "postpublish": "icake clean"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://matt-hickford@github.com/matt-hickford/imgurgitate.git"
  },
  "engines": {
    "node": "~0.8.0"
  },
  "dependencies": {
    "http-get": "~0.3.13",
    "request": "~2.9.100",
    "underscore": "~1.3.1",
    "optimist": "~0.3.1"
  },
  "devDependencies": {
    "iced-coffee-script": "~1.2.0u",
    "prettyjson": "~0.3.1",
    "wrench": "~1.3.7"
  },
  "optionalDependencies": {}
}
