{
  "author": "Mirth Hickford",
  "name": "imgurgitate",
  "description": "Download Imgur albums to disk. Download all of a Redditor's albums to disk.",
  "version": "0.1.2",
  "homepage": "https://github.com/hickford/imgurgitate",
  "bugs": {
    "url": "https://github.com/hickford/imgurgitate/issues"
  },
  "keywords": [
    "imgur",
    "album",
    "reddit",
    "download"
  ],
  "directories": {
    "lib": "./lib"
  },
  "main": "./lib/imgurgitate.js",
  "bin": {
    "imgurgitate": "./lib/imgurgitate.js"
  },
  "scripts": {
    "prepublish": "icake build",
    "postpublish": "icake clean"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/hickford/imgurgitate.git"
  },
  "engines": {
    "node": "0.8 - 0.10"
  },
  "dependencies": {
    "request": "*",
    "underscore": "*",
    "optimist": "*",
    "http-get": "*"
  },
  "devDependencies": {
    "iced-coffee-script": ">=1.4.0c",
    "wrench": "*"
  },
  "optionalDependencies": {}
}
